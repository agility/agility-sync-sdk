{
  "name": "@agility/content-sync",
  "version": "1.1.9",
  "description": "JavaScript SDK for synchronizing content from Agility CMS",
  "main": "dist/agility-sync-sdk.node.js",
  "browser": "dist/agility-sync-sdk.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:types && npm run build:bundle",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
    "build:bundle": "webpack --config webpack.config.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "docs": "jsdoc -c jsdoc.json",
    "generate-docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agility/agility-sync-sdk.git"
  },
  "author": "Agility CMS",
  "license": "MIT",
  "contributors": [
    "Joel Varty",
    "James Vidler",
    "Joshua Isaac"
  ],
  "bugs": {
    "url": "https://github.com/agility/agility-sync-sdk/issues"
  },
  "homepage": "https://github.com/agility/agility-sync-sdk#readme",
  "dependencies": {
    "@agility/content-fetch": "^1.0.0",
    "dotenv": "^8.2.0",
    "proper-lockfile": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/node": "^18.11.9",
    "@vitest/coverage-c8": "^0.25.3",
    "@vitest/ui": "^0.25.3",
    "agility-jsdoc-template": "1.0.3",
    "babel-loader": "^9.1.3",
    "chai": "^4.3.10",
    "jsdoc": "^4.0.0",
    "jsdom": "^26.1.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "vitest": "^0.25.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
}
