{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.ts(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "agility-jsdoc-template": {
      "staticFiles": {
        "include": ["./docs/static"]
      }
    }
  },
  "opts": {
    "template": "node_modules/agility-jsdoc-template",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  }
} 